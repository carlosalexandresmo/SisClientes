<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity repository-class="Unimed\ClienteBundle\Entity\ClienteRepository" name="Unimed\ClienteBundle\Entity\Cliente">
    <id name="id" type="integer" column="id">
      <generator strategy="AUTO"/>
    </id>
    <field name="nome" type="string" column="nome" length="100"/>
    <field name="dtnasc" type="date" column="dtnasc"/>
    <field name="endereco" type="string" column="endereco" length="255"/>
    <field name="email" type="string" column="email" length="100"/>
    <field name="telefone" type="string" column="telefone" length="11"/>
    <field name="celular" type="string" column="celular" length="10"/>
    <field name="matricula" type="string" column="matricula" length="100"/>
    <field name="login" type="string" column="login" length="10"/>
    <field name="senha" type="string" column="senha" length="8"/>
  </entity>
</doctrine-mapping>
